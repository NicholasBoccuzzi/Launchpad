<!DOCTYPE html>
<html>
  <head>
    <title>Launchpad</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <% if current_user %>
    <h1> Current User: <%= current_user.username %> </h1>
    <% end %>

    <%= yield %>
  </body>
</html>
